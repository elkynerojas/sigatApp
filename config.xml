<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Sigat</name>
    <description>
        Sistema de Información para la gestión de Alertas Tempranas Plan Nacional de la Mosca de la Fruta
    </description>
    <author email="rojaselkin1@gmail.com" href="http://190.121.143.211">
        Elkin E. Rojas L.
    </author>
    <icon src="res/icono-mosca.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-sqlite" spec="^1.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
